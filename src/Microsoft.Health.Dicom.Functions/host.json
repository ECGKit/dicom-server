{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "indexing": {
    "add": {
      "batchSize": 1000,
      "parallelBatches": 10
    }
  },
  "OrchestrationsCleanup": {
    "RuntimeStatuses": [ "Completed" ],
    "MinimumAgeDays": 14
  }
}
